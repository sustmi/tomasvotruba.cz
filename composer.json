{
    "name": "tomasvotruba/website",
    "require": {
        "php": "^7.1",
        "symplify/statie": "^2.5",
        "nette/utils": "^2.4",
        "nette/finder": "^2.4",
        "J7mbo/twitter-api-php": "^1.0"
    },
    "require-dev": {
        "symplify/easy-coding-standard": "^2.5",
        "phpunit/phpunit": "^6.4",
        "nikic/php-parser": "v4.0.0alpha1 as 3.1.1",
        "sebastian/object-graph": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "TomasVotruba\\Website\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TomasVotruba\\Website\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs check bin src tests",
        "fix-cs": "vendor/bin/ecs check bin src tests --fix"
    }
}
